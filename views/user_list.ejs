<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>
<ol>
    <% results.forEach(function(user) {%>
        <li>
            <%= user.name %>
        </li>
    <% }); %>
</ol>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>
<%if (previous) { %>
    <a class="nav-buttons" href="<%= previous %>"><i class="fas fa-arrow-left"></i></a>
<% } %>
</body>
<%- include('_footer') -%>
</html>